where <!-- MP EQPHNS( --><script id=mpeq0027s1>
	MPSetEqnAttrs('eq0027','',3,[[33,8,2,-1,-1],[44,11,4,-1,-1],[58,14,4,-1,-1],[50,12,4,-1,-1],[68,16,4,-1,-1],[84,20,6,-1,-1],[142,34,9,-2,-2]])
