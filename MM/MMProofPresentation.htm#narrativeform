	MPEquation()
</script><!-- )MP EQPHNS --><span
style='mso-spacerun:yes'> </span>for which <!-- MP EQPHNS( --><script id=mpeq0008s1>
	MPSetEqnAttrs('eq0008','',3,[[38,10,2,-1,-1],[51,14,3,-1,-1],[63,17,3,-1,-1],[56,15,4,-1,-1],[77,20,4,-1,-1],[94,25,5,-1,-1],[157,41,9,-2,-2]])
</script><![if !ie]><span id=mpnneq0008ph class=MPNNCode
