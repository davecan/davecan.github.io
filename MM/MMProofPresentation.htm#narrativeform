	MPEquation()
</script><!-- )MP EQPHNS -->.
But <!-- MP EQPHNS( --><script id=mpeq0009s1>
	MPSetEqnAttrs('eq0009','',3,[[75,10,2,-1,-1],[101,14,3,-1,-1],[125,17,3,-1,-1],[112,15,4,-1,-1],[151,20,4,-1,-1],[186,25,5,-1,-1],[313,41,9,-2,-2]])
