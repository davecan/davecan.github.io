Cell["Find entries generated by an integer", "Subsubsection",
 CellChangeTimes->{{3.554413773657159*^9, 3.554413802764824*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"fv", "[", 
   RowBox[{"gl_List", ",", "n_Integer"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"nn", ":=", "n"}], ";", 
    RowBox[{"ggl", ":=", 
     RowBox[{"{", "nn", "}"}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Sow", "[", "1", "]"}], ";", 
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"MemberQ", "[", 
           RowBox[{"ggl", ",", 
            RowBox[{"cf", "[", "nn", "]"}]}], "]"}]}], ",", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Sow", "[", "nn", "]"}], ";", 
            RowBox[{"Sow", "[", 
             RowBox[{"cf", "[", "nn", "]"}], "]"}]}], ")"}], ";", 
          RowBox[{"nn", "=", 
           RowBox[{"cf", "[", "nn", "]"}]}]}]}], "]"}]}], ")"}], "//", 
     "Reap"}]}], ")"}], 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Sow", "[", "1", "]"}], " ", "first", " ", "to", " ", "keep", " ",
     "the", " ", "output", " ", "from", " ", "being", " ", "empty"}], 
   "*)"}]}]], "Input",
