	MPSetEqnAttrs('eq0023','',3,[[6,8,2,-1,-1],[8,10,2,-1,-1],[11,14,3,-1,-1],[9,12,2,-1,-1],[12,16,3,-1,-1],[15,19,3,-1,-1],[26,33,7,-2,-2]])
