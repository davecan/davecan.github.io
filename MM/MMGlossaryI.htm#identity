</script><![if !ie]><span id=mpnneq0004ph class=MPNNCode
	><img border=0 name=mpeq0004ph src='&{DSMP.gEmptySrc};' height=1
		width='&{DSMP.gPlaceholderWidth};'
>&nbsp;</span><![endif]><script id=mpeq0004s2>
	MPEquation()
</script><!-- )MP EQPHNS --></p><!-- MP EQRA( --><script id=mpeq0004s1>
	MPSetEqnAttrs('eq0004','',3,[[82,14,5,-1,-1],[106,19,6,-1,-1],[135,24,7,-1,-1],[119,22,7,-1,-1],[160,27,8,-1,-1],[200,35,11,-1,-1],[335,56,17,-2,-2]]);
